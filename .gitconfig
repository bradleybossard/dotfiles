[user]
	name = Bradley Bossard
	email = bradleybossard@gmail.com
[color]
	ui = true
[diff]
	tool = vimdiff
[merge]
  tool = /Users/bradleybossard/scripts/diffwrap.sh 
[difftool "gvimdiff"]
	path = mvim
[push]
  #default = simple
  default = tracking
[pull]
     rebase = true
[branch]
      autosetuprebase = always
[core]
	autocrlf = input
	editor = vim
[alias]
	co = checkout
	st = status 
  br = branch
  ci = commit
  aci = commit -a --message
  logp = log -p
  logpretty = log --pretty=oneline
  lol = log --oneline --graph --decorate
  incoming = log origin/master ^master
  outgoing = log master ^origin/master
  unstage = reset HEAD --
  last = log -1 HEAD
[credential]
	helper = store
